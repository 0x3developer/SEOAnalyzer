    @{
    ViewBag.Title = "Home Page";
}

<nav class="navbar navbar-light bg-light">
    <div class="container-fluid">
        <a class="navbar-brand" href="#">
            <img alt="" width="30" height="24" class="d-inline-block align-text-top" src="data:image/svg+xml;base64," />
            Code Analyzer
        </a>
    </div>
</nav>
<div class="card mt-5">
    <div></div>
    <div class="card-body">
        <ul class="nav nav-tabs" id="myTab" role="tablist">
            <li class="nav-item" role="presentation">
                <button class="nav-link active" id="seo-tab" data-bs-toggle="tab" data-bs-target="#seo" type="button" role="tab" aria-controls="seo" aria-selected="true">SEO Analysis</button>
            </li>
            <li class="nav-item" role="presentation">
                <button class="nav-link" id="seo-instructions-tab" data-bs-toggle="tab" data-bs-target="#seo-instructions" type="button" role="tab" aria-controls="seo-instructions" aria-selected="false">Instructions</button>
            </li>
        </ul>
        <div class="tab-content p-2 pt-3" id="myTabContent">
            <div class="tab-pane fade show active" id="seo" role="tabpanel" aria-labelledby="seo-tab">
                <div class="row mb-3">
                    <div class="col">
                        <div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="seo-input-options" id="input-text-option" value="input-text-group" checked>
                                <label class="form-check-label" for="input-text-option">Plain Text</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="seo-input-options" id="input-url-option" value="input-url-group">
                                <label class="form-check-label" for="input-url-option">URL</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mb-3">
                    <div class="col">
                        <div class="mb-3">
                            <div class="row" id="stop-word-input-group">
                                <div class="col-3">
                                    <div class="form-group">
                                        <input class="form-check-input" type="checkbox" id="chk-exclude-stop-words">
                                        <label class="form-check-label">Exclude Stop Words</label>
                                    </div>
                                </div>
                                <div class="col-3">
                                    <div class="form-group">
                                        <input class="form-check-input" type="checkbox" id="chk-exclude-body">
                                        <label class="form-check-label">Exclude Html Body</label>
                                    </div>
                                </div>
                                <div class="col-3">
                                    <div class="form-group">
                                        <input class="form-check-input" type="checkbox" id="chk-exclude-meta">
                                        <label class="form-check-label">Exclude Search Header Meta</label>
                                    </div>
                                </div>
                                <div class="col-3">
                                    <div class="form-group">
                                        <input class="form-check-input" type="checkbox" id="chk-exclude-numbers">
                                        <label class="form-check-label">Exclude Numbers</label>
                                    </div>
                                </div>
                                <div class="col-3">
                                    <div class="form-group">
                                        <input class="form-check-input" type="checkbox" id="chk-find-external-links">
                                        <label class="form-check-label">Find External Links</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id='input-groups'>
                            <div id='input-text-group'>
                                <label for="input-text" class="form-label">Plain Text Input</label>
                                <textarea class="form-control" id="input-text"></textarea>
                            </div>
                            <div class="mb-3" id='input-url-group'>
                                <label for="input-url" class="form-label">Url <small>example: http://www.example.com.my</small></label>
                                <input type="url" class="form-control" id="input-url">
                            </div>
                        </div>
                    </div>
                </div>
                <button type="button" class="btn btn-primary" id="btn-analysis">Analysis</button>
                <span class="m-3" id='loading-icon'></span>
                <div id="formError" class="form-text text-danger"></div>

                <div id="result-container">
                    <div class="mt-5">
                        <div class="container table-responsive py-5">
                            <table id="seo-data-table" class="table">
                                <thead class="thead-dark">
                                    <tr>
                                        <th scope="col">#</th>
                                        <th scope="col">Word</th>
                                        <th scope="col">Count</th>
                                        <th scope="col">Meta</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>

                        <h5>External Links</h5>
                        <div class="container table-responsive py-2">
                            <table id="seo-link-table" class="table">
                                <thead class="thead-dark">
                                    <tr>
                                        <th scope="col">#</th>
                                        <th scope="col">Link</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div id="result-links">

                    </div>
                </div>
            </div>
            <div class="tab-pane fade" id="seo-instructions" role="tabpanel" aria-labelledby="seo-instructions-tab">Here Is SEO Instructions</div>
            
        </div>
    </div>
</div>